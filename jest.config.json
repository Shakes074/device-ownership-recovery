{
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.(t|j)sx?$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!qrcode.react)"
  ],
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  },
  "setupFilesAfterEnv": [
    "<rootDir>/src/setupTests.js"
  ]
}